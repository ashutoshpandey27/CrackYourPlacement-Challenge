class Solution {
public:
    void moveZeroes(vector<int>& nums) {

        int n = nums.size() , nonZero = 0;

        for(int i = 0 ; i < n ; i++)
        {
            if(nums[i])
            {
                swap(nums[nonZero] , nums[i]);

                nonZero++;
            }
        }
        
        
    }
};
